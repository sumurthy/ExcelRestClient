    <div class="container">
      <div class="header clearfix">
        <nav>
          <ul class="nav nav-pills pull-right">
            <li role="presentation" class="active"><a href="/">Home</a></li>
          </ul>
        </nav>
        <h3 class="text-muted">Excel REST Testing App</h3>
      </div>

      <h3><span class="label label-primary">Update worksheet, ID: <%= id %> </span></h3>
      <form class="navbar-form navbar-left" method="post" action="/updateworksheetroute">
          <div class="form-group">
           <input type="text" name="sheetname" class="form-control" placeholder="New Name">
           <input type="text" name="position" class="form-control" placeholder="New Position">
           <input type="hidden" name="id" value="<%= id %>">
          </div>
          <button type="submit" class="btn btn-default">Submit</button>
      </form>


   
